ty": [], "colors": [], "convertedManaCost": 1.0, "count": 1, "edhrecRank": 2769, "edhrecSaltiness": 0.35, "finishes": ["nonfoil"], "foreignData": [{"identifiers": {"scryfallId": "bcdc3a81-1a4b-4038-98e6-7ff259d617eb"}, "language": "German", "name": "Bibliothek von Leng", "text": "Übergehe die Abwerf-Phase Deines Spielzuges. Zwingt Dich irgendeine Karte oder ein Effekt, Karten abzuwerfen, kannst Du Dir aussuchen, ob Du die so abgeworfenen Karten auf den Friedhof oder oben auf Deine Bibliothek legst. Wenn Du eine zufällige Karte abwerfen musst, schau sie Dir gut an, bevor Du Dich entscheidest, wohin Du sie abwirfst.", "type": "Artefakt"}, {"identifiers": {"scryfallId": "43471615-140a-4ee8-9f76-1b9293220d3b"}, "language": "French", "name": "Bibliothèque de Leng"}, {"identifiers": {"scryfallId": "2a3ea7a9-d837-4269-abef-9ea5a8a688f6"}, "language": "Italian", "name": "Biblioteca di Leng"}], "frameVersion": "1993", "hasFoil": false, "hasNonFoil": true, "identifiers": {"cardsphereId": "39197", "mcmId": "5790", "mtgjsonV4Id": "5a310962-d298-5a00-812f-67f5bfa802d7", "multiverseId": "1122", "scryfallCardBackId": "0aeebaf5-8c7d-4636-9e82-8c27447861f7", "scryfallId": "0634ab23-4691-4c77-9b8f-bfd9d99b31a1", "scryfallIllustrationId": "dde78c10-fa53-4478-8c26-1b84565f41c7", "scryfallOracleId": "867def48-4be8-4056-bcf1-d6b00450b9a3", "tcgplayerProductId": "1475"}, "isFoil": false, "isReprint": true, "language": "English", "layout": "normal", "legalities": {"commander": "Legal", "duel": "Legal", "legacy": "Legal", "oathbreaker": "Legal", "oldschool": "Legal", "pauper": "Legal", "paupercommander": "Legal", "predh": "Legal", "premodern": "Legal", "vintage": "Legal"}, "manaCost": "{1}", "manaValue": 1.0, "name": "Library of Leng", "number": "261", "originalText": "You must skip the discard phase of your turn. If a card forces you to discard, you may choose to discard to top of your library rather than to graveyard. If discard is random, you may look at card before deciding where to discard it.", "originalType": "Artifact", "printings": ["2ED", "30A", "3ED", "4BB", "4ED", "5ED", "CED", "CEI", "FBB", "LEA", "LEB", "ME4", "SUM"], "purchaseUrls": {"tcgplayer": "https://mtgjson.com/links/ebddfdcb71cbbc05"}, "rarity": "uncommon", "rulings": [{"date": "2004-10-04", "text": "If more than one card is discarded due to a single effect, the Library allows you to decide whether or not to use it on each of the cards. You get to decide the order the cards are placed on the library if more than one goes there."}, {"date": "2004-10-04", "text": "Since the card goes directly to the library, the card is not revealed unless the spell or ability requiring the discard specifically says it is."}, {"date": "2004-10-04", "text": "The ability applies any time a spell or ability has you discard as part of its effect. It does not matter if you or your opponent control the spell or ability. The discard is forced because it is an effect."}, {"date": "2004-10-04", "text": "The ability replaces the normal discard action with a discard action that puts the card on the library instead of the graveyard."}, {"date": "2004-10-04", "text": "The discard triggers anything else that triggers on discards."}, {"date": "2004-10-04", "text": "This effect has no effect on the cards being put into the graveyard from a library, because they are not \"discarded\"."}, {"date": "2004-10-04", "text": "You can look at a randomly discarded card before deciding where it goes."}, {"date": "2004-10-04", "text": "You can't use the Library of Leng ability to place a discarded card on top of your library when you discard a card as a cost, because costs aren't effects."}, {"date": "2009-10-01", "text": "If multiple effects modify your hand size, apply them in timestamp order. For example, if you put Null Profusion (an enchantment that says your maximum hand size is two) onto the battlefield and then put Library of Leng onto the battlefield, you'll have no maximum hand size. However, if those permanents entered in the opposite order, your maximum hand size would be two."}], "setCode": "3ED", "sourceProducts": {"nonfoil": ["4ebfc88d-eea7-599e-ba79-c86020d1c1e2", "9b04b8ff-5402-5553-a531-323a38d8c4a4"]}, "subtypes": [], "supertypes": [], "text": "You have no maximum hand size.\nIf an effect causes you to discard a card, discard it, but you may put it on top of your library instead of into your graveyard.", "type": "Artifact", "types": ["Artifact"], "uuid": "074dae50-a4a5-5063-88fa-821196c03ec4"}], "name": "Enchantress", "releaseDate": "1997-04-01", "sealedProductUuids": null, "sideBoard": [], "type": "Shandalar Enemy Deck"}}