uel": "Legal", "legacy": "Legal", "oathbreaker": "Legal", "vintage": "Legal"}, "manaCost": "{1}{W}{U}{B}", "manaValue": 4.0, "name": "Varina, Lich Queen", "number": "1355", "originalReleaseDate": "2023-10-02", "power": "4", "printings": ["2X2", "C18", "PZ2", "SLD"], "purchaseUrls": {"tcgplayer": "https://mtgjson.com/links/1cab826e24d26ef1"}, "rarity": "mythic", "rulings": [{"date": "2018-07-13", "text": "The number of cards you discard and the amount of life you gain are both equal to the number of Zombies you attack with, even if you draw a different number of cards somehow."}, {"date": "2018-07-13", "text": "Varina’s first ability counts the number of Zombies you attacked with, even if some of those Zombies leave combat before the triggered ability resolves."}], "securityStamp": "triangle", "setCode": "SLD", "sourceProducts": {"foil": ["93acea5b-f69c-5769-ae00-bdfd7e24e086"], "nonfoil": ["85e48b5a-9ad2-53df-a28b-fce59b7a3f41"]}, "subsets": ["Evil Dead"], "subtypes": ["Zombie", "Wizard"], "supertypes": ["Legendary"], "text": "Whenever you attack with one or more Zombies, draw that many cards, then discard that many cards. You gain that much life.\n{2}, Exile two cards from your graveyard: Create a tapped 2/2 black Zombie creature token.", "toughness": "4", "type": "Legendary Creature — Zombie Wizard", "types": ["Creature"], "uuid": "5665e1a5-73a0-59e0-93c8-1af0cd1d5c5a"}, {"artist": "Warren Mahy", "artistIds": ["002c739e-aa1f-4c1d-921b-37cb2d1b1c5b"], "availability": ["paper"], "borderColor": "borderless", "colorIdentity": [], "colors": [], "convertedManaCost": 0.0, "count": 1, "edhrecRank": 330, "edhrecSaltiness": 1.12, "finishes": ["nonfoil", "foil"], "flavorName": "Cabin of the Dead", "flavorText": "Scream as your nightmares come true.", "foreignData": [], "frameEffects": ["inverted"], "frameVersion": "2015", "hasFoil": true, "hasNonFoil": true, "identifiers": {"cardsphereFoilId": "113944", "cardsphereId": "113914", "mtgjsonV4Id": "d8577108-aeb1-5a8e-b544-646a84