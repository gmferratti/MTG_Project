s": [], "toughness": "1", "type": "Creature — Human Soldier", "types": ["Creature"], "uuid": "10afa9c9-8d12-55c9-a4d1-bd9a5d6eaf46"}, {"artist": "Dave Dorman", "artistIds": ["9235458b-ce92-45bd-a430-a72ecd8e6176"], "availability": ["mtgo", "paper"], "boosterTypes": ["default"], "borderColor": "white", "colorIdentity": ["W"], "colors": ["W"], "convertedManaCost": 2.0, "count": 4, "edhrecRank": 23315, "finishes": ["nonfoil"], "flavorText": "The turning of the tide always begins with one soldier's decision to head back into the fray.", "foreignData": [{"flavorText": "Das Schlachtenglück kehrt immer mit dem einen Soldaten zurück, der es sich anders überlegt und sich wieder in den Kampf stürzt.", "identifiers": {"multiverseId": "98552", "scryfallId": "c2328ca6-0c62-4338-9cec-7d8b4b5080ab"}, "language": "German", "multiverseId": 98552, "name": "Ruhmsucher", "type": "Kreatur — Mensch, Soldat"}, {"flavorText": "El cambio de la suerte siempre comienza con la decisión de un soldado de regresar a la lucha.", "identifiers": {"multiverseId": "99992", "scryfallId": "a6122185-91d3-4ab9-b981-1d2a174e3258"}, "language": "Spanish", "multiverseId": 99992, "name": "Buscador de gloria", "type": "Criatura — Soldado humano"}, {"flavorText": "Un seul soldat retourne au combat, et l'espoir change de camp.", "identifiers": {"multiverseId": "98192", "scryfallId": "a214410b-fa17-40be-8bb5-59ca8b6c1b49"}, "language": "French", "multiverseId": 98192, "name": "Chercheur de gloire", "type": "Créature : humain et soldat"}, {"flavorText": "Le sorti della battaglia sono destinate a mutare quando un soldato decide di tornare a gettarsi nella mischia.", "identifiers": {"multiverseId": "98912", "scryfallId": "7ea0f0b3-62a0-4954-b279-0bed32b4c629"}, "language": "Italian", "multiverseId": 98912, "name": "Cacciatore di Gloria", "type": "Creatura — Soldato Umano"}, {"flavorText": "戦場の風向きは、いつだって一人の兵士が争いの場に踵を返すことから変わり始めるものだ。", "identifiers": {"multiverseId": "99272", "scryfallId": "6147720d-7ecb-4263-9925-a5e0efd5164c"}, "language": "Japanese", "multiverseId": 99272, "name": "栄光の探求者", "type": "クリーチャー — 人間・兵士"}, {"flavorText": "A mudança da maré sempre tem início com a decisão de um soldado de voltar para a luta.", "identifiers": {"multiverseId": "99632", "scryfallId": "a9179913-2027-4038-a06a-d80e6ea5cf05"}, "language": "Portuguese (Brazil)", "multiverseId": 99632, "name": "Perseguidor da Glória", "type": "Criatura — Soldado Humano"}, {"flavorText": "Переломный момент всегда начинается с решения одного из солдат броситься в бой.", "identifiers": {"multiverseId": "109903", "scryfallId": "65a55ca8-36ef-449e-8ce1-334981f8b9fd"}, "language": "Russian", "multiverseId": 109903, "name": "Искатель славы", "type": "Существо — Человек Солдат"}, {"flavorText": "战局的逆转总是起于某位士兵带头返身奋战。", "identifiers": {"multiverseId": "97832", "scryfallId": "0813c5b4-3f93-451c-8a18-618f20711097"}, "language": "Chinese Simplified", "multiverseId": 97832, "name": "荣耀追求者", "type": "生物～人类／士兵"}], "frameVersion": "2003", "hasFoil": false, "hasNonFoil": true, "identifiers": {"cardsphereFoilId": "3583", "cardsphereId": "3584", "mcmId": "12377", "mcmMetaId": "2363", "mtgjsonV4Id": "206df96d-9399-58fd-a953-ef3d5f688c08", "mtgoFoilId": "22700", "mtgoId": "22699", "multiverseId": "83107", "scryfallCardBackId": "0aeebaf5-8c7d-4636-9e82-8c27447861f7", "scryfallId": "4979748a-4820-4c6a-b4b0-1bfb46e2817a", "scryfallIllustrationId": "370ad855-81de-4bf9-b9df-074b6ab03bd2", "scryfallOracleId": "55c60b29-52c5-428c-9229-11ed0e26bd71", "tcgplayerProductId": "12670"}, "isFoil": false, "isReprint": true, "language": "English", "layout": "normal", "legalities": {"commander": "Legal", "duel": "Legal", "legacy": "Legal", "modern": "Legal", "oathbreaker": "Legal", "pauper": "Legal", "paupercommander": "Legal", "penny": "Legal", "pioneer": "Legal", "predh": "Legal", "premodern": "Legal", "vintage": "Legal"}, "manaCost": "{1}{W}", "manaValue": 2.0, "name": "Glory Seeker", "number": "17", "originalType": "Creature - Human Soldier"