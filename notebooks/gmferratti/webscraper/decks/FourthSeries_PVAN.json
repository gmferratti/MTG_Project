ame": "Tawnos", "number": "405", "originalText": "You may play artifact, creature, and enchantment spells any time you could play an instant.", "originalType": "Vanguard - Character", "printings": ["PVAN"], "promoTypes": ["instore"], "purchaseUrls": {"tcgplayer": "https://mtgjson.com/links/a8474e86e8379f79"}, "rarity": "rare", "setCode": "PVAN", "subtypes": [], "supertypes": [], "text": "You may cast artifact, creature, and enchantment spells as though they had flash.", "type": "Vanguard", "types": ["Vanguard"], "uuid": "b306d05f-bc1b-5421-a3c0-20239834f126"}, {"artist": "Rebecca Guay", "artistIds": ["f8f662fa-d597-46a3-afb2-91d6e13243e2"], "availability": ["paper"], "borderColor": "black", "colorIdentity": [], "colors": [], "convertedManaCost": 0.0, "count": 1, "finishes": ["nonfoil"], "flavorText": "Voice of the Argoth forest, defender of its creatures, and enforcer of its laws, Titania is literally an aspect of the forest itself. Her power and her very existence are linked to the land: when the forest is healthy, Titania is ascendant. When Argoth suffers, Titania also bleeds.", "foreignData": [], "frameVersion": "1993", "hand": "+2", "hasFoil": false, "hasNonFoil": true, "identifiers": {"mcmId": "14985", "mtgjsonV4Id": "ab6f6f54-6a55-5722-8db5-81115604b159", "multiverseId": "12333", "scryfallCardBackId": "222b7a3b-2321-4d4c-af19-19338b134971", "scryfallId": "3b7de05c-e5aa-421a-bf16-b16fef8fc6ef", "scryfallIllustrationId": "95702f00-b847-486c-89f2-4fea4506cab6", "scryfallOracleId": "024cfe2e-d3c0-4341-bb70-67664ed1595d", "tcgplayerProductId": "21631"}, "isFoil": false, "isOversized": true, "isStarter": true, "language": "English", "layout": "vanguard", "legalities": {}, "life": "-5", "manaValue": 0.0, "name": "Titania", "number": "406", "originalText": "You may play an additional land each of your turns.", "originalType": "Vanguard - Character", "printings": ["PVAN"], "promoTypes": ["instore"], "purchaseUrls": {"tcgplayer": "https://mtgjson.com/links/6732dcb18ed01