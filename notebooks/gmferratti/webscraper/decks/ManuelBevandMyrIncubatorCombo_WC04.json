colorIdentity": ["U"], "colors": ["U"], "convertedManaCost": 3.0, "count": 3, "edhrecRank": 1833, "edhrecSaltiness": 0.1, "finishes": ["nonfoil", "signed"], "flavorText": "Lymph, the fluid essence of blinkmoths, is prized by wizards for the rush of intellect it provides.", "foreignData": [], "frameVersion": "2003", "hasFoil": false, "hasNonFoil": true, "identifiers": {"mcmId": "249163", "mtgjsonV4Id": "ac926ab3-f3c0-5c7b-92fa-19e332f31222", "scryfallCardBackId": "896307e5-1c5d-49c0-8fb5-8d1059bc1115", "scryfallId": "e45e19b6-cef3-42ea-b53b-f1fead9ec8f8", "scryfallIllustrationId": "48dddb94-e86c-447e-95a7-64e892cb639f", "scryfallOracleId": "939e6f71-185e-41f2-9d54-72cce06f1dce", "tcgplayerProductId": "174397"}, "isAlternative": true, "isFoil": false, "isReprint": true, "isStarter": true, "language": "English", "layout": "normal", "legalities": {}, "manaCost": "{2}{U}", "manaValue": 3.0, "name": "Thirst for Knowledge", "number": "mb53", "printings": ["2XM", "BRC", "C18", "CMR", "DDF", "F08", "HOP", "JMP", "MMA", "MRD", "NEO", "PIP", "PRM", "TD2", "WC04"], "purchaseUrls": {"tcgplayer": "https://mtgjson.com/links/a2baa6e42d31a8bf"}, "rarity": "uncommon", "rulings": [{"date": "2020-11-10", "text": "You can discard either one artifact card or two cards which may or may not be artifacts. If you really want to, you can discard two artifact cards."}], "setCode": "WC04", "signature": "Manuel Bevand", "sourceProducts": {"nonfoil": ["9de1b4d3-13f7-5914-af25-eb19c23dd8e0"]}, "subtypes": [], "supertypes": [], "text": "Draw three cards. Then discard two cards unless you discard an artifact card.", "type": "Instant", "types": ["Instant"], "uuid": "d27098a4-65e2-55af-ab2a-3f8385f651ac", "variations": ["79a472cc-0eb2-56b1-b2da-7bae2f22d0bb"]}, {"artist": "Glen Angus", "artistIds": ["7be3b9e4-de9b-4a3d-884e-e67b9681d409"], "availability": ["paper"], "borderColor": "gold", "colorIdentity": ["U"], "colors": ["U"], "convertedManaCost": 3.0, "count": 3, "edhrecRank": 412, "edhrecSaltiness